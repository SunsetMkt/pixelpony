{
   "version": "2.0.0",
   "configurations": [
      {
         "type": "node",
         "request": "attach",
         "name": "attach and auto-re-attach",
         "processId": "${command:PickProcess}",
         "restart": true,
         "protocol": "inspector",
      },
      {
         "type": "node",
         "request": "launch",
         "name": "script:launch",
         "program": "${workspaceFolder}/package/script/dist/serve.js",
      },
   ]
}
